<?xml version="1.0" encoding="utf-8"?>

<Keyboard xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:ask="http://schemas.android.com/apk/res-auto"
    android:keyWidth="8.3333%p"
    android:horizontalGap="0px"
    android:verticalGap="0px"
    android:keyHeight="@dimen/key_height"
    >

    <Row>
        <Key android:codes="q" android:popupCharacters="0" android:keyEdgeFlags="left"/>
        <Key android:codes="w" android:popupCharacters="1"/>
        <Key android:codes="e" android:popupCharacters="2"/>
        <Key android:codes="r" android:popupCharacters="3"/>
        <Key android:codes="t" android:popupCharacters="4"/>
        <Key android:codes="y" android:popupCharacters="5"/>
        <Key android:codes="u" android:popupCharacters="6"/>
        <Key android:codes="i" android:popupCharacters="7"/>
        <Key android:codes="o" android:popupCharacters="8"/>
        <Key android:codes="p" android:popupCharacters="9"/>
        <Key android:codes="ő" android:popupCharacters="•\u2026"/>
        <Key android:codes="ú" android:popupCharacters="ù" android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="a" android:popupCharacters="á" android:keyEdgeFlags="left"/>
        <Key android:codes="s" android:popupCharacters="+"/>
        <Key android:codes="d" android:popupCharacters="-"/>
        <Key android:codes="f" android:popupCharacters="["/>
        <Key android:codes="g" android:popupCharacters="]"/>
        <Key android:codes="h" android:popupCharacters="("/>
        <Key android:codes="j" android:popupCharacters=")"/>
        <Key android:codes="k" android:popupCharacters="%"/>
        <Key android:codes="l" android:popupCharacters="&amp;#"/>
        <Key android:codes="é" android:popupCharacters="€$"/>
        <Key android:codes="á" android:popupCharacters="à"/>
        <Key android:codes="ű" android:popupCharacters="\\|" android:keyEdgeFlags="right"/>
    </Row>

    <Row>
        <Key android:codes="z" android:popupCharacters="=§" android:keyEdgeFlags="left"/>
        <Key android:codes="x" android:popupCharacters="*"/>
        <Key android:codes="c" android:popupCharacters="/"/>
        <Key android:codes="v" android:popupCharacters="\@"/>
        <Key android:codes="b" android:popupCharacters="{"/>
        <Key android:codes="n" android:popupCharacters="}"/>
        <Key android:codes="m" android:popupCharacters="&lt;"/>
        <Key android:codes="ö" android:popupCharacters="&gt;"/>
        <Key android:codes="ü" android:popupCharacters="_"/>
        <Key android:codes="ó" android:popupCharacters="ò"/>
        <Key android:codes="@integer/key_code_delete" android:keyEdgeFlags="right" android:isRepeatable="true" android:keyWidth="16.7%p"/>
    </Row>

    <!-- Generic bottom row -->
    <Row android:rowEdgeFlags="bottom">
        <Key android:codes="@integer/key_code_shift" android:isModifier="true" android:isSticky="true" android:keyEdgeFlags="left"/>
        <Key android:codes="í" android:popupCharacters="ì"/>
        <Key android:codes="@integer/key_code_quick_text" ask:isFunctional="true" />
        <Key android:codes="63" ask:isFunctional="true" android:popupCharacters="-"/>
        <Key android:codes="44" ask:isFunctional="true" android:popupCharacters=";"/>
        <Key android:codes="32" ask:longPressCode="@integer/key_code_voice_input" ask:isFunctional="true" android:keyWidth="25.1%p"/>
        <Key android:codes="46" ask:isFunctional="true" android:popupCharacters=":"/>
        <Key android:codes="!" ask:isFunctional="true" android:popupCharacters="&#34;\'"/>
        <Key android:codes="10" ask:longPressCode="@integer/key_code_settings" android:keyWidth="16.6666%p" android:keyEdgeFlags="right"/>
    </Row>
    
</Keyboard>
